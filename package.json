{
  "name": "mui-datepicker-with-range",
  "version": "0.1.7",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "keywords": ["react", "mui", "datepicker"],
  "babel": {
    "presets": [
      "@babel/preset-react"
    ]
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "react": "^16.4.0",
    "react-dom": "^16.8.6",
